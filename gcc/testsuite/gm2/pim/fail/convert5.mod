MODULE convert5 ;

PROCEDURE Min (a, b: CARDINAL) : CARDINAL ;
BEGIN
   IF a<b
   THEN
      RETURN( a )
   ELSE
      RETURN( b )
   END
END Min ;

PROCEDURE foo (c: CARDINAL) : INTEGER ;
BEGIN
(*   RETURN( -VAL(INTEGER, VAL(CARDINAL, MAX(INTEGER))+1) ) *)
   RETURN( -VAL(INTEGER, VAL(CARDINAL, 2147483647)+1) )
END foo ;

BEGIN
   IF foo(2)=2
   THEN
   END
END convert5.
